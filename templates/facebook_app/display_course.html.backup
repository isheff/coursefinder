			<tr>
				<td height="10" class="Title">
					Comments				</td>
			</tr>
			<tr>
				<td height="1" bgcolor="#808080">				</td>
			</tr>
			<tr>
				<td height="30">
					<table width="470" border="0" cellpadding="0" class="Rating_item">
						<tr>
							<td width="235" height="30">
								FB friends comments							</td>
							<td width="235" height="30">
								FB public comments							</td>
						</tr>
						<tr>
							<td>
								Not Yet Avaliable							</td>
							<td>
								Not Yet Avaliable							</td>
						</tr>
					</table>				</td>
			</tr>
			<tr>
				<td height="10">&nbsp;				</td>
			</tr>
			<tr>
				<td height="10">
				<button class="searchback" onclick="location.href='index'"/>
				</td>
			</tr>
			<tr>
				<td height="10">&nbsp;				</td>
			</tr>
		</table>
		<fb:login-button autologoutlink="true"></fb:login-button>
		<div id="fb-root">
		</div>
		<script>
			window.fbAsyncInit = function() {
				FB.init({appId: '{{ facebook_app_id }}', status: true, cookie: true,
					xfbml: true});
				FB.Event.subscribe('{% if current_user %}auth.logout{% else %}auth.login{% endif %}', function(response) {
					window.location.reload();
				});
			};
			(function() {
				var e = document.createElement('script');
				e.type = 'text/javascript';
				e.src = document.location.protocol + '//connect.facebook.net/en_US/all.js';
				e.async = true;
				document.getElementById('fb-root').appendChild(e);
			}());
		</script>
</body>
</html>
