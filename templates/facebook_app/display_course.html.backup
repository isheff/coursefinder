	</head>
	<body onload="MM_preloadImages('/media/searchback_link.png')">
		<table width="510" border="0" cellpadding="0">
			<tr>
				<td>
					<table width="510" border="0" cellpadding="0">
						<tr>
							<td>
								<a href="/"><img src="/media/Caltech_namecopy.png" alt="Caltech_name" width="350" height="100" border="0" /></a> </td>
							<td>
								<img src="/media/Caltech_logo.png" alt="Caltech_logo" width="100" height="100" />							</td>
						</tr>
					</table>				</td>
			</tr>
			<tr>
			  <td height="40">
			  <button class="searchback" onclick="location.href='/'"/>
			  
			  </td>
		  </tr>
			<tr>
				<td height="40">
				<table width="500" border="0" cellpadding="0" style="border:1px #3B5998  solid">
                  <tr>
                    <td width="95" rowspan="2">
					<table width="95" border="0" cellpadding="0">
                      <tr>
                        <td height="85"  ><span ><img src="/media/dept/{{course.department|first}}.png" alt="course_tmp" width="85" height="85" align="absmiddle" style="border:1px gray  solid"/></span></td>
                      </tr>
                    </table></td>
                    <td width="10" rowspan="3">&nbsp;</td>
                    <td colspan="2" ><span class="Course_title"> {% for department in course.department %}{{department}} {% endfor %} </span><span class="Course_title">{{ course.name }}</span></td>
                  </tr>
                  <tr>
                    <td colspan="2"><span class="Course_detail">{{course.description}}</span></td>
                  </tr>
                  <tr>
			  <td width="95"> {% if Overall_Rating %} 
				  <div aling="center" class="rating_bar_small">
					  <div style="width:{{Overall_Rating}}%">
					  </div>
				  </div> {% endif %}
	  </td>
                    <td width="80" ><span class="Course_instructor">Instructors:</span></td>
                    {% for teacher in teachers %}
					<td width="280" ><span class="Course_instructor">{{ teacher.title }} {{ teacher.name }} </span></td>
					{% endfor %}                  </tr>
                </table></td>
			</tr>
			<tr>
				<td>					</td>
			</tr>
			<tr>
				<td height="10">&nbsp;				</td>
			</tr>
			<tr>
				<td>
					<table width="500" border="0" cellpadding="0" class="Rating_item">
						<tr>
							<td width="100">							</td>
							<td width="200">
								Algorithmic Estimate							</td>
							<td width="50">							</td>
							<td width="121">
								My Rating							</td>
						</tr>
						<tr>
							<td width="120" height="25">
								Overall:							</td>
							<td width="200">
								{% if Overall_Rating %}
								<div class="rating_bar" ><div style="width:{{Overall_Rating}}%">	</div>	</div>	
								<div >	({{Overall_Rating_value|floatformat:1}})</div>	
								{% else %}
									Not Yet Avaliable
								{% endif %}						  </td>
							<td width="50">&nbsp;							</td>
							<td width="200" height="25" valign="middle">
								<div>
									<script type="text/javascript" language="javascript">
										moonStarStars("Overall_Rating", {{ User_Overall_Rating }});
									</script>
								</div>						  </td>
						</tr>
						{% for teacher in teachers %}
							<tr>
								<td width="120"  height="11" class="Rating_item">
									Instructor: </td>
								<td width="200" rowspan="2">
									{% if teacher.rating %}
										<div class="rating_bar">
											<div style="width:{{teacher.rating}}%">											</div>
										</div>	
									{% else %}
										Not Yet Avaliable
									{% endif %}							  </td>
								<td width="50" rowspan="2">
								<td width="200" height="25" rowspan="2">
									<div>
										<script type="text/javascript" language="javascript">
											moonStarStars("Teacher_{{teacher.id}}_Rating", {{teacher.user_rating}});
										</script>
									</div>							  </td>
							</tr>
							<tr>
							  <td  height="12" class="Rating_item"><span class="style3">{{teacher.title}} {{teacher.name}}</span> </td>
					  </tr>
						{% endfor %}
						<tr>
							<td width="120"  height="25" class="Rating_item">
								Grading:							</td>
							<td width="200">		
								{% if Grading_Rating %}
									<div class="rating_bar">
										<div style="width:{{Grading_Rating}}%">										</div>
									</div>		
								{% else %}
									Not Yet Avaliable
								{% endif %}						  </td>
							<td width="50">&nbsp;							</td>
							<td width="200" height="25">
								<div>
									<script type="text/javascript" language="javascript">
										moonStarStars("Grading_Rating", {{User_Grading_Rating}});
									</script>
								</div>						  </td>
						</tr>
				  </table>				</td>
			</tr>
			<tr>
			  <td height="15">
			  <p  class="Rating_item">
							Number of hours spent/ week on this class (at least):						</p>
						<script type="text/javascript">
							moonStars("Hours", {{Hours}}, "time_bar_gray.png", "time_bar_blue.png", [1,'','','',5,'','','','',10,'','','','',15,'','','','',20]);
						</script>
						<p  class="Rating_item">
							Grade in this class (at least):						</p>
						<script type="text/javascript">
							moonStars("Grade", {{Grade}}, "25px-grey-check.png", "25px-green-check.png", ["F", "D", "C", "B", "A"]);
						</script>
			  </td>
		  </tr>
			<tr><td height="15">&nbsp;</td></tr>
			<tr>
				<td>
					<div style="width: 510px; background-color: #EDEFF4;">
						<script type="text/javascript">
							mooComment("Course_Comment", "Course Comment [optional]", "{{Course_Comment}}", {{Course_Comment_Privacy}});
							{% for teacher in teachers %}
								mooComment("Teacher_{{ teacher.id }}_Comment", "Instructor Comment: {{teacher.title}} {{teacher.name}} [optional]", "{{teacher.course_comment}}", {{teacher.course_comment_privacy}});
							{% endfor %}
						</script>
						
					</div>				</td>
			</tr>
			<tr>
				<td height="20">&nbsp;				</td>
			</tr>
