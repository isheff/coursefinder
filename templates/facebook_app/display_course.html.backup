        <td width="235" height="30" class="Rating_item"><div align="center">Grade</div></td>
        <td width="235" height="30" class="Rating_item"><div align="center">Time-spent</div></td>
      </tr>
      <tr>
        <td><div align="center"></div></td>
        <td><div align="center"></div></td>
      </tr>
    </table></td>
  </tr>
  <tr>
    <td height="40">&nbsp;</td>
  </tr>
